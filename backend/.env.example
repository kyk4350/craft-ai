# Backend Environment Variables

# API Keys
GEMINI_API_KEY=your_gemini_api_key_here
STABILITY_API_KEY=your_stability_api_key_here
VOYAGE_AI_API_KEY=your_voyage_ai_key_here

# AI 모델 설정
# IMAGE_PROVIDER: mock (개발 초기), stability (개발 중), nanobanana (최종)
IMAGE_PROVIDER=mock
# GEMINI_MODEL: gemini-2.5-flash (기본), gemini-2.5-pro (고품질)
GEMINI_MODEL=gemini-2.5-flash

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/contentcraft

# Redis
REDIS_URL=redis://localhost:6379/0

# Qdrant Vector DB
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your_qdrant_api_key_here

# Application
ENVIRONMENT=development
DEBUG=True
